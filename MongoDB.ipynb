   "source": [
    "Interestingly the result is not a collection: https://docs.mongodb.com/manual/reference/method/db.collection.find/index.html"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "colab": {
     "base_uri": "https://localhost:8080/"
    },
    "id": "tPvttfKn2mbj",
